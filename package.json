		"@typescript-eslint/parser": "^5.12.1",
		"alistair": "^1.5.6",
		"eslint": "^8.10.0",
		"eslint-config-next": "12.1.0",
		"eslint-config-xo": "^0.40.0",
		"eslint-config-xo-react": "^0.26.0",
		"eslint-config-xo-typescript": "^0.50.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"prettier": "^2.5.1",
		"prisma": "^5.7.1",
		"ts-node": "^10.5.0",
		"tsx": "^4.7.0",
		"typescript": "^4.5.5"
